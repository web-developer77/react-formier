{
  "root": true,
  "extends": "eslint-config-formio",
  "parser": "babel-eslint",
  "rules": {
    "max-len": 0,
    "max-depth": [1, 5],
    "max-statements": 0,
    "no-unused-vars": 0,
    "object-curly-spacing": [1, "always"]
  },
  "globals": {
    "window": true,
    "require": true,
    "module": true,
    "global": true,
    "fetch": true
  }
}